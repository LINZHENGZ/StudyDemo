<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="sgg.study.sggdemo">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.SGGdemo">
        <activity
                android:name=".TabLayout.activity.TabLayout"
                android:theme="@style/Theme.AppCompat.Light.NoActionBar"
                android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
       </activity>

        <activity
                android:name=".MainActivity"
                android:exported="true"/>

        <activity android:name=".keepalive.activity.KeepActivity"
                  android:excludeFromRecents="true"
                  android:taskAffinity="sgg.study.sggdemo.keep"
                  android:theme="@style/KeepTheme"/>
        <service android:name=".keepalive.service.ForegroundService"/>
    </application>


</manifest>